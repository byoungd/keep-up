{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "KeepUp",
  "version": "0.1.0",
  "identifier": "com.keepup.desktop",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:3000/en/app",
    "beforeBuildCommand": "",
    "frontendDist": "../../cowork/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "KeepUp",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
