{
  "name": "@ku0/cli",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Keep-Up CLI tooling",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "keepup": "dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "dependencies": {
    "@ku0/agent-runtime": "workspace:*",
    "@ku0/agent-runtime-control": "workspace:*",
    "@ku0/agent-runtime-core": "workspace:*",
    "@ku0/ai-core": "workspace:*",
    "@ku0/tooling-session": "workspace:*",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.0"
  }
}
