{
  "name": "@ku0/agent-runtime-persistence",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Checkpoint persistence and time-travel utilities",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./checkpoint": {
      "types": "./dist/checkpoint/index.d.ts",
      "default": "./dist/checkpoint/index.js"
    },
    "./timetravel": {
      "types": "./dist/timetravel/index.d.ts",
      "default": "./dist/timetravel/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "dependencies": {
    "better-sqlite3": "^9.6.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.0"
  }
}
