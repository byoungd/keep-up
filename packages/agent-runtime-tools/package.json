{
  "name": "@ku0/agent-runtime-tools",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Agent Runtime tool registry, tool servers, and integrations",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./tools": {
      "types": "./src/tools/index.ts",
      "default": "./dist/tools/index.js"
    },
    "./tools/mcp": {
      "types": "./src/tools/mcp/index.ts",
      "default": "./dist/tools/mcp/index.js"
    },
    "./tools/core": {
      "types": "./src/tools/core/index.ts",
      "default": "./dist/tools/core/index.js"
    },
    "./tools/core/bash": {
      "types": "./src/tools/core/bash.ts",
      "default": "./dist/tools/core/bash.js"
    },
    "./tools/core/completion": {
      "types": "./src/tools/core/completion.ts",
      "default": "./dist/tools/core/completion.js"
    },
    "./tools/core/file": {
      "types": "./src/tools/core/file.ts",
      "default": "./dist/tools/core/file.js"
    },
    "./tools/computer": {
      "types": "./src/tools/computer/index.ts",
      "default": "./dist/tools/computer/index.js"
    },
    "./browser": {
      "types": "./src/browser/index.ts",
      "default": "./dist/browser/index.js"
    },
    "./plugins": {
      "types": "./src/plugins/index.ts",
      "default": "./dist/plugins/index.js"
    },
    "./skills": {
      "types": "./src/skills/index.ts",
      "default": "./dist/skills/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "dependencies": {
    "@ku0/agent-runtime-control": "workspace:*",
    "@ku0/agent-runtime-core": "workspace:*",
    "@ku0/agent-runtime-persistence": "workspace:*",
    "@ku0/agent-runtime-sandbox": "workspace:*",
    "@ku0/agent-runtime-telemetry": "workspace:*",
    "@ku0/agent-runtime-vision": "workspace:*",
    "@ku0/agent-toolkit-rs": "workspace:*",
    "@ku0/tool-gateway-rs": "workspace:*",
    "@ku0/core": "workspace:*",
    "@ku0/db": "workspace:*",
    "@ku0/diff-rs": "workspace:*",
    "@ku0/gitignore-rs": "workspace:*",
    "@ku0/sandbox-rs": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "diff": "^8.0.3",
    "gray-matter": "^4.0.3",
    "playwright": "^1.51.0"
  },
  "devDependencies": {
    "@types/diff": "^8.0.0",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.0"
  }
}
