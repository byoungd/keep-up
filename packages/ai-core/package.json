{
  "name": "@ku0/ai-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./providers": {
      "types": "./src/providers/index.ts",
      "default": "./dist/providers/index.js"
    },
    "./context": {
      "types": "./src/context/index.ts",
      "default": "./dist/context/index.js"
    },
    "./resilience": {
      "types": "./src/resilience/index.ts",
      "default": "./dist/resilience/index.js"
    },
    "./performance": {
      "types": "./src/performance/index.ts",
      "default": "./dist/performance/index.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@ku0/tokenizer-rs": "workspace:*",
    "@ku0/model-fabric-rs": "workspace:*",
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/openai": "^3.0.12",
    "ai": "^6.0.39",
    "cockatiel": "^3.2.1",
    "js-tiktoken": "^1.0.21",
    "langfuse": "^3.38.6",
    "zod": "^4.3.5"
  }
