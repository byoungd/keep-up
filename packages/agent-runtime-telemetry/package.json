{
  "name": "@ku0/agent-runtime-telemetry",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Agent Runtime telemetry, metrics, and logging",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./logging": {
      "types": "./src/logging/index.ts",
      "default": "./dist/logging/index.js"
    },
    "./telemetry": {
      "types": "./src/telemetry/index.ts",
      "default": "./dist/telemetry/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3"
  }
}
